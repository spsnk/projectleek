{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "brace-style": [1, "allman"]
  }
}
